#pragma once

namespace wpi {
  
class math {
 public:
  static constexpr double e = 2.718281828459045235360287471352662498L;
  static constexpr double log2e = 1.442695040888963407359924681001892137L;
  static constexpr double log10e = 0.434294481903251827651128918916605082L;
  static constexpr double pi = 3.141592653589793238462643383279502884L;
  static constexpr double inv_pi = 0.318309886183790671537767526745028724L;
  static constexpr double inv_sqrtpi = 0.564189583547756286948079451560772586L;
  static constexpr double ln2 = 0.693147180559945309417232121458176568L;
  static constexpr double ln10 = 2.302585092994045684017991454684364208L;
  static constexpr double sqrt2 = 1.414213562373095048801688724209698078L;
  static constexpr double sqrt3 = 1.732050807568877293527446341505872366L;
  static constexpr double inv_sqrt3 = 0.577350269189625764509148780501957456L;
  static constexpr double egamma = 0.577215664901532860606512090082402431L;
  static constexpr double phi = 1.618033988749894848204586834365638117L;
};

}  // namespace wpi
