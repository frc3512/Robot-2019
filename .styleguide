cppHeaderFileInclude {
  \.h$
  \.hpp$
  \.inl$
  \.inc$
}

cppSrcFileInclude {
  \.cpp$
}

generatedFileExclude {
  src/main/cpp/fmt/
  src/main/include/fmt/
}

